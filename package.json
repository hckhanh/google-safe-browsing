    "@vitest/coverage-v8": "2.1.9",
    "@vitest/eslint-plugin": "1.1.31",
    "eslint": "9.21.0",
    "eslint-plugin-perfectionist": "4.9.0",
    "prettier": "3.5.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1",
    "vitest": "2.1.9"
